{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    
      "rewrites": [
        {
          "source": "/socket.io",
          "destination": "https://livetracking-backend.vercel.app/socket.io"
        }
      ]
    ,
    
  
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}
